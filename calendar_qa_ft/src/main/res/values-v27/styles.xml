<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Calendar theme. -->
    <style name="CalendarTheme" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/calendarColorPrimary</item>
        <item name="colorPrimaryDark">@color/calendarColorAccent</item>
        <item name="colorAccent">@color/calendarColorAccent</item>
        <item name="android:windowLightNavigationBar">true</item>
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>
        <item name="android:navigationBarColor">@android:color/background_light</item>
        <item name="android:navigationBarDividerColor">@color/disabled_stroke</item>
    </style>
</resources>